{
    "name": "Goal Reminder",
    "description": "Reminding the user about the inspiration things they said before and empowering them",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": ["alarms", "notifications", "storage", "webNavigation"],
    "background":{
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_title": "Be Awesome",
        "default_popup": "popup.html"
    }
}